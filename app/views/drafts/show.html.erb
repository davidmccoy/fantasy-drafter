<div id="draft" data-draft="<%= @draft.id %>" data-user="<%= current_user.id %>" data-team="<%= current_user.team(@draft.league)&.id %>">
  <% case @draft.league.draft_type %>
  <% when 'snake' %>
    <%= render 'snake_draft' %>
  <% when 'pick_x' %>
    <%= render 'pick_x_draft' %>
  <% end %>
</div>
